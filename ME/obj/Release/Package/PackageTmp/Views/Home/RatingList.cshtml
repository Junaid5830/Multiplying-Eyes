<div>
  <div class="row">
    <div class="col-xs-12">
      <div class="box">
        <div class="box-header">
          <h3 class="box-title">Rating List</h3>
        </div>
        <!-- /.box-header -->
        <div class="box-body">
          <table id="Rating" class="table table-bordered table-striped">
            <thead>
              <tr>
                <th>Sr#</th>
                <th>Score</th>
                <th>RatedBy</th>
                <th>RatinOn</th>
              </tr>
            </thead>
            <tbody>
              @if (ViewBag.Rating != null)
              {
                int count = 1;
                foreach (var loop in ViewBag.Rating)
                {

                  <tr>
                    <td>@count</td>
                    <td>@loop.Score</td>
                    <td>@loop.RatedBy</td>
                    <td>@loop.RatedOn</td>
                    @*<td>
                        <a style="margin-right: 5px;color:red" href="DeleteUser?id=@loop.UserId">Delete</a>
                        <span>
                            <i name="@loop.Name" userId="@loop.UserId" email="@loop.Email" password="@loop.Password" city="@loop.City" desig="@loop.Designation" usertype="@loop.UserTypeName" class="EditUser fa fa-edit" style="color: #337ab7;cursor: pointer;"></i>
                          </span>
                          <span style="margin-left: 10px;">
                            <i userId="@loop.UserId" class="RemoveUser fa fa-trash-o" style="color: red;cursor: pointer;"></i>
                          </span>
                      </td>*@
                  </tr>
                  count++;
                }
              }
            </tbody>
          </table>
        </div>
        <!-- /.box-body -->
      </div>
    </div>
  </div>
</div>
<script>
  $(document).ready(function () {
    $('#Rating').DataTable();
  });
</script>






